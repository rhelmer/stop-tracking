<?xml version='1.0' encoding='utf-8'?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>stop-tracking@test.mozilla.org</em:id>
    <em:name>Stop Tracking</em:name>
    <em:description>Enables built-in tracking protection and enables reporting for broken pages</em:description>
    <em:version>0.0.2</em:version>
    <em:creator>Robert Helmer</em:creator>

    <em:bootstrap>true</em:bootstrap>
    <em:type>2</em:type>

    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>33.0</em:minVersion>
        <em:maxVersion>47.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Android -->
    <em:targetApplication>
      <Description>
        <em:id>{aa3c5121-dab2-40e2-81ca-7ea25febc110}</em:id>
        <em:minVersion>33.0</em:minVersion>
        <em:maxVersion>47.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
